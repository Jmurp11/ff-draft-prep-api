{
   "name": "server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "~2.4.2",
      "@types/jest": "~24.0.16",
      "@types/node": "~12.6.8",
      "@types/uuid": "~3.4.5",
      "gql2ts": "~1.10.1",
      "jest": "~24.8.0",
      "nodemon": "~1.19.1",
      "prettier": "~1.18.2",
      "ts-jest": "~24.0.2",
      "ts-node": "8.3.0",
      "typescript": "3.5.3"
   },
   "dependencies": {
      "bcryptjs": "~2.4.3",
      "chalk": "~2.4.2",
      "graphql": "~14.4.2",
      "graphql-import": "~0.7.1",
      "graphql-request": "~1.8.2",
      "graphql-tools": "~4.0.5",
      "graphql-yoga": "^1.18.1",
      "pg": "~7.11.0",
      "reflect-metadata": "~0.1.13",
      "tslint": "~5.18.0",
      "tslint-config-prettier": "~1.18.0",
      "typeorm": "0.2.18",
      "uuid": "~3.3.2"
   },
   "scripts": {
      "start": "NODE_ENV=production ts-node --files src/index.ts",
      "dev": "NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
      "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts",
      "test": "NODE_ENV=test jest --detectOpenHandles"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "bail": true,
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
